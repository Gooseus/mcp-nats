# NATS Connection Configuration
# Copy this to .env and fill in your values

# NATS server URL (default: nats://localhost:4222)
NATS_URL=nats://localhost:4222

# Basic auth (optional)
NATS_USER=
NATS_PASS=

# Token auth (optional)
NATS_TOKEN=

# Credentials file path for NGS/Synadia Cloud (optional)
NATS_CREDS_PATH=

# TLS settings (optional)
# NATS_TLS_CA_FILE=
# NATS_TLS_CERT_FILE=
# NATS_TLS_KEY_FILE=
